/*Bootstrap */
#content {
margin-right:10px;
margin-left:10px;
}

#side {
float:right;
width:350px;
margin-left:10px;
}

#side #sidebox{
background-color:#EFF7FF;
padding: 20px;
border: 1px solid #5F99CF;
margin-bottom:20px;
}



#welcome-panel{
  margin-bottom:0px;
  margin-top:0px;
  padding:0px;
  background-color:#EFF7FF;
}

#title h1, #title a{
  margin:0px;
}

#comment {
  padding:20px;

}

#comment #entry {
  padding-left:160px;

}



#upvote:hover, #downvote:hover{
   box-shadow: 0px 0px 3px 5px #f2e1f2; 

}

#midcol #score{
text-align: center;
}


#midcol{
width: 4ex;
margin-right: 4px;
margin-left: 7px;
padding-top:10px;

}

#midcol #upvote, #midcol #downvote {
margin: 8px 0px 0px 0px
cursor: pointer;
width: 15px;
height: 14px;
display: block;
margin-left: auto;
margin-right: auto;
}



#title .tuna{
padding-top:35px;
color:#49AFCD;
}

#title .voodoo{
color:#4C4C4C;
padding-top:35px;
padding-left:20px;
}


  #welcome-panel #title{
  padding-bottom:10px;
  padding-top:10px;
  padding-left:10px;
  }


body {
padding-top:0px;
font: 20px "Helvetica Neue", Helvetica, Arial, sans-serif;
font-weight: 300;
background-color: white;
background-image: none;
}
